
@{
    ViewData["Title"] = "Tourist - Reservations";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="padding-30">
    <h2>Reservations</h2>
    <table id="tblData" class="table table-striped table-bordered text-center" style="width:100%">
        <thead class="thead-dark">
            <tr class="table-info">
                <th>Trip Name</th>
                <th>Booking Number</th>
                <th>Booking Date</th>
                <th>Booking Status</th>
                <th>Trip</th>
                <th>Reservation</th>
            </tr>
        </thead>

    </table>
</div>

@section Scripts{
    <script>
        $(".table-info th").css("text-align", "center")
    </script>
    <script src="~/js/Dashboard/tourist.js"></script>
}